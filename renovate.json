{
  "extends": [
    "config:base",
    ":automergeTypes"
  ],
  "packageRules": [
    {
      "matchDepTypes": ["devDependency"],
      "matchUpdateTypes": ["patch", "minor"],
      "automerge": true
    },
    {
      "matchPackageNames": ["telegraf"],
      "allowedVersions": "<=4.11.2 >4.12.2"
    }
  ]
}
