{
  "extends": [
    "config:base",
    ":automergeTypes",
    ":automergeMinor"
  ],
  "packageRules": [
    {
      "matchDepTypes": ["devDependencies"],
      "matchUpdateTypes": ["patch", "minor"],
      "automerge": true,
      "groupName": "dev dependencies"
    },
    {
      "matchPackageNames": ["telegraf"],
      "allowedVersions": "<=4.11.2 >4.12.2"
    }
  ]
}
